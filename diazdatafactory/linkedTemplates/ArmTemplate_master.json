{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "diazdatafactory"
		},
		"AzureDatabricks_DataFactory_Cobre_Plata_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_DataFactory_Cobre_Plata\""
		},
		"AzureDatabricks_DataFactory_Plata_Oro_accessToken": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accessToken\"de \"AzureDatabricks_DataFactory_Plata_Oro\""
		},
		"Conector_ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"Conector_ADLS\""
		},
		"AzureDatabricks_DataFactory_Cobre_Plata_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1001-000307-nx8rvusx"
		},
		"AzureDatabricks_DataFactory_Plata_Oro_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1001-000307-nx8rvusx"
		},
		"Conector_ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://kennydiazacertus.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "diazdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks_DataFactory_Cobre_Plata_accessToken": {
						"value": "[parameters('AzureDatabricks_DataFactory_Cobre_Plata_accessToken')]"
					},
					"AzureDatabricks_DataFactory_Plata_Oro_accessToken": {
						"value": "[parameters('AzureDatabricks_DataFactory_Plata_Oro_accessToken')]"
					},
					"Conector_ADLS_accountKey": {
						"value": "[parameters('Conector_ADLS_accountKey')]"
					},
					"AzureDatabricks_DataFactory_Cobre_Plata_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_DataFactory_Cobre_Plata_properties_typeProperties_existingClusterId')]"
					},
					"AzureDatabricks_DataFactory_Plata_Oro_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks_DataFactory_Plata_Oro_properties_typeProperties_existingClusterId')]"
					},
					"Conector_ADLS_properties_typeProperties_url": {
						"value": "[parameters('Conector_ADLS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}